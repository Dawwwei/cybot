                    }
                },
                "ban": {
                    "name": "ban",
                    "description": "Sunucudan bir üyeyi yasakla",
                    "options": {
                        "member": {
                            "name": "üye",
                            "description": "Yasaklamak istediğiniz üye"
                        },
                        "reason": {
                            "name": "sebep",
                            "description": "Üyenin yasaklanma sebebi"
                        }
                    },
                    "responses": {
                        "success": "{{member}} başarıyla sunucudan yasaklandı. Sebep: {{reason}}",
